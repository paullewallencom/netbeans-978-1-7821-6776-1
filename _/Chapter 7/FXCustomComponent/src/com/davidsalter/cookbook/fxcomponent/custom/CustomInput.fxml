<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.HBox" alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" >
  <children>
    <Label fx:id="label" text="Label" HBox.hgrow="ALWAYS">
      <HBox.margin>
        <Insets right="8.0" />
      </HBox.margin>
    </Label>
    <TextField fx:id="text" prefWidth="100.0" />
  </children>
  <padding>
    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
  </padding>
</fx:root>
